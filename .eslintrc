{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": 1,
    "comma-dangle": [ 2, "never" ],
    "comma-style": [ 2, "first" ],
    "array-bracket-spacing": [ 2, "always"],
    "no-unused-vars": 1,
    "no-nested-ternary": 0,
    "no-else-return": 0,
    "react/prefer-stateless-function": [ 1 ],
    "react/forbid-prop-types": 1,
    "react/prop-types": [ 1 ],
    "react/jsx-filename-extension": [ 1, { "extensions": [ ".js", ".jsx" ] } ],
    "space-unary-ops": [
      2, {
        "words": true,
        "nonwords": false,
        "overrides": {
          "!": true
        }
      }
    ]
  }
}

// vim:ft=json
